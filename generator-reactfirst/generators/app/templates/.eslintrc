{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "_": true,
    "$": true,
    "__uri": true,
    "__inline": true,
    "define": true,
    "console": true,
    "Device": true,
    "logger": true,
    "QMTV": true,
    "api": true,
    "api3": true,
    "Mitt": true,
    "cacheMixin": true,
    "lv": true,
    "imageResize": true,
    "util": true,
    "adRender": true,
    "swfobject": true,
    "serverNow": true,
    "store": true,
    "Vue": true,
    "moment": true
  },
  "rules": {
    "spaced-comment": 0,
    "max-len": [
      1,
      120
    ],
    "object-curly-spacing": 0,
    "no-alert": 1,
    "no-eval": 1,
    "no-console": 0,
    "indent": 0,
    "dot-location": [
      2,
      "property"
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-empty": 0,
    "eol-last": 0,
    "no-multi-spaces": 1,
    "no-unused-vars": [
      1,
      {
        "args": "none"
      }
    ],
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 1,
    "curly": 1,
    "no-shadow": 2,
    "dot-notation": 2,
    "no-undef": 2,
    "block-scoped-var": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ]
  }
}